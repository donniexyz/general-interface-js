 <description><![CDATA[a sample bar chart of type 'Stacked']]></description>
 <onBeforeDeserialize><![CDATA[]]></onBeforeDeserialize>
 <onAfterDeserialize><![CDATA[]]></onAfterDeserialize>
  <object type="jsx3.chart.BarChart">
   <dynamics />
   <variants jsxleft="0" jsxtop="0" jsxwidth="400" jsxheight="300" borderWidth="1" borderAlpha="1" alpha="1" loadState="1" seriesOverlap="0.3" categoryCoverage="0.65" jsxrelativeposition="1" />
   <strings jsxname="stackedBarChart" titlePlacement="top" legendPlacement="right" borderColor="#999999" type="stacked" jsxbgcolor="#FFFFFF" jsxxml="&lt;data jsxid=&quot;jsxroot&quot;&gt;&lt;record cat=&quot;North&quot; s1=&quot;15&quot; s2=&quot;10&quot;/&gt;&lt;record cat=&quot;South&quot; s1=&quot;22&quot; s2=&quot;15&quot;/&gt;&lt;record cat=&quot;East&quot; s1=&quot;15&quot; s2=&quot;30&quot;/&gt;&lt;record cat=&quot;West&quot; s1=&quot;5&quot; s2=&quot;15&quot;/&gt;&lt;record cat=&quot;Central&quot; s1=&quot;8&quot; s2=&quot;20&quot;/&gt;&lt;/data&gt;" dataPadding="14 10 6 6" />
    <object type="jsx3.chart.Legend">
