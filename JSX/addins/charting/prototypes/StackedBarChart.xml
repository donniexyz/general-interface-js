     <strings jsxname="title" jsxtext="Stacked Bar Chart" jsxbgcolor="#666666" jsxpadding="4 0 0 0" jsxfontname="Arial" jsxfontweight="bold" jsxcolor="#FFFFFF" jsxtextalign="center" />
    </object>
    <object type="jsx3.chart.LinearAxis">
