<data jsxid="jsxroot">
  <record include="master.xml" group="object"/>
  <record include="master.xml" group="position"/>

  <record jsxid="plotchart" group="1" jsxtext="PlotChart">
    <record jsxid="maxPointRadius" jsxtext="Max Radius"
        jsxtip="Sets the maximum point radius to display on this chart. Larger points are constrained to this value."
        docgetter="getMaxPointRadius" docsetter="setMaxPointRadius"
        docdefault="DEFAULT_MAX_POINT_RADIUS (&lt;code&gt;jsx3.chart.PlotChart.DEFAULT_MAX_POINT_RADIUS&lt;/code&gt;)"
        eval="1"
        disallow="[^0-9]"
        jsxmask="jsxtext"/>
    <record jsxid="magnitudeMethod" jsxtext="Magnitude Method"
        jsxtip="Sets the method for converting magnitude values to point size."
        docgetter="getMagnitudeMethod" docsetter="setMagnitudeMethod"
        docdefault="radius (&lt;code&gt;jsx3.chart.PlotChart.MAG_RADIUS&lt;/code&gt;)"
        eval="0" nullable="0"
        jsxmask="jsxselect">
      <enum jsxid="jsx3.chart.PlotChart.MAG_RADIUS" jsxtext="radius"/>
      <enum jsxid="jsx3.chart.PlotChart.MAG_DIAMETER" jsxtext="diameter"/>
      <enum jsxid="jsx3.chart.PlotChart.MAG_AREA" jsxtext="area"/>
    </record>
  </record>

  <record include="master.xml" group="blockx"/>
  <record include="master.xml" group="background"/>
  <record include="master.xml" group="chart"/>
  <record include="master.xml" group="interaction_menuonly"/>

</data>
