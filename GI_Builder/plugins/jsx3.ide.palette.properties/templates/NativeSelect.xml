<data jsxid="jsxroot">
  <record include="master.xml" group="object"/>
  <record include="master.xml" group="position"/>

  <record jsxid="jsx3select" group="1" jsxtext="Select">
    <record jsxid="jsxmultiple" jsxtext="Selection Mode"
        eval="1"
        jsxtip="Sets whether this select allows multiple or single selection."
        docgetter="getMultiple" docsetter="setMultiple"
        docdefault="Single (&lt;code&gt;jsx3.Boolean.FALSE&lt;/code&gt;)"
        jsxmask="jsxselect">
      <enum jsxid="jsx3.Boolean.FALSE" jsxtext="Single"/>
      <enum jsxid="jsx3.Boolean.TRUE" jsxtext="Multiple"/>
    </record>
    <record jsxid="jsxsize" jsxtext="Rows"
        eval="1"
        jsxtip="The number of option rows to show."
        docgetter="setRows" docsetter="getRows"
        docdefault="1"
        disallow="[^0-9]"
        jsxmask="jsxtext"/>
    <record include="master.xml" group="form" children="1"/>
    <record include="master.xml" group="jsxrequired" children="1"/>
  </record>

  <record include="master.xml" group="blockx-xml"/>
  <record include="master.xml" group="font"/>

  <record include="master.xml" group="box_nobg">
    <record include="master.xml" group="jsxdisabledbgcolor" children="1"/>
  </record>

  <record include="master.xml" group="css"/>
  <record include="master.xml" group="interaction"/>
  <record include="master.xml" group="access"/>
  <record include="master.xml" group="metadata"/>
</data>
