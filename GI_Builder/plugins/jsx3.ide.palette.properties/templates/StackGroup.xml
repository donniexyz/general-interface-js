<data jsxid="jsxroot">
  <record include="master.xml" group="object"/>

  <record jsxid="position" group="1" jsxtext="Positioning">
    <record include="master.xml" path="/data/record[@jsxid='position']/record[@jsxid='jsxrelativeposition']"/>
  </record>


  <record jsxid="jsx3stackgroup" group="1" jsxtext="StackGroup">
    <record jsxid="jsxbarsize" jsxtext="Bar Size"
        eval="1"
        jsxtip="Sets the height/width of the caption bar for each child stack, depending upon orientation"
        docgetter="getBarSize" docsetter="setBarSize"
        docdefault="&lt;code&gt;jsx3.gui.StackGroup.DEFAULTBARSIZE&lt;/code&gt;. See the API documentation for this component."
        disallow="[^0-9]"
        jsxmask="jsxtext"/>
    <record jsxid="jsxorientation" jsxtext="Orientation"
        eval="1"
        jsxtip="Sets whether this stack group is rendered top-over (--) or side-by-side (|)."
        docgetter="getOrientation" docsetter="setOrientation"
        docdefault="Top-over (--) (&lt;code&gt;jsx3.gui.StackGroup.ORIENTATIONV&lt;/code&gt;)"
        jsxmask="jsxselect">
      <enum jsxid="jsx3.gui.StackGroup.ORIENTATIONV" jsxtext="Top-over (--)"/>
      <enum jsxid="jsx3.gui.StackGroup.ORIENTATIONH" jsxtext="Side-by-side (|)"/>
    </record>
    <record jsxid="jsxselectedindex" jsxtext="Active Stack Index"
        eval="1"
        jsxtip="Sets the zero-based index of the active tab."
        docgetter="getSelectedIndex"
        docdefault="&lt;code&gt;0&lt;/code&gt;"
        disallow="[^0-9]"
        jsxmask="jsxtext"/>
  </record>

  <record jsxid="box" group="1" jsxtext="Box">
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxbgcolor']"/>
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxbg']"/>
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxborder']"/>
  </record>
  <record include="master.xml" group="css_noclass"/>
  <record include="master.xml" group="access"/>
  <record include="master.xml" group="metadata"/>
</data>
