<data jsxid="jsxroot">
  <record include="master.xml" group="object"/>
  <record include="master.xml" group="position"/>

  <record jsxid="jsx3datepicker" group="1" jsxtext="DatePicker">
    <record include="master.xml" group="form_novalue" children="1"/>
    <record include="master.xml" group="jsxrequired" children="1"/>
    <record jsxid="jsxdateformat" jsxtext="Date Format"
        jsxtip="Sets the date format used to format and parse the value of this DatePicker in its text field. Supported tokens include y, M, d, and E. See the API documentation for &lt;code&gt;jsx3.util.DateFormat&lt;/code&gt;."
        docgetter="getFormat" docsetter="setFormat"
        docdefault="Determined by the locale of the server containing this DatePicker."
        eval="0"
        disallow="&quot;"
        jsxtype="jsxdateformat"
        jsxmask="jsxtext"
        jsxexecute="objJSX.setFormat(vntValue);objJSX.repaint();"/>
    <record jsxid="jsxfirstweekday" jsxtext="Week Start"
        jsxtip="Sets the first day of the week."
        docgetter="getFirstDayOfWeek" docsetter="setFirstDayOfWeek"
        docdefault="Determined by the locale of the server containing this DatePicker."
        eval="1"
        jsxmask="jsxselect">
      <enum jsxid="0" jsxtext="{date.day[0]}"/>
      <enum jsxid="1" jsxtext="{date.day[1]}"/>
      <enum jsxid="2" jsxtext="{date.day[2]}"/>
      <enum jsxid="3" jsxtext="{date.day[3]}"/>
      <enum jsxid="4" jsxtext="{date.day[4]}"/>
      <enum jsxid="5" jsxtext="{date.day[5]}"/>
      <enum jsxid="6" jsxtext="{date.day[6]}"/>
    </record>
    <record jsxid="jsxnulllabel" jsxtext="Null Label"
        eval="0"
        jsxtip="The label to show in this DatePicker when its value is null."
        docgetter="getDefaultText" docsetter="setDefaultText"
        docdefault="The string representation of the date format of this DatePicker."
        jsxmask="jsxtext"/>
  </record>

  <record include="master.xml" group="font"/>

  <record jsxid="box_tree" group="1" jsxtext="Box">
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxbgcolor']"/>
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxbg']"/>
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxmargin']"/>
    <record include="master.xml" path="/data/record[@jsxid='box']/record[@jsxid='jsxborder']"/>
    <record include="master.xml" group="jsxdisabledbgcolor" children="1"/>
    <record jsxid="jsxicon" jsxtext="Icon"
        eval="0"
        jsxtip="The persistent image that will trigger the display of the calendar. This image will be positioned at the right-most edge of the control when painted.  The recommended image size is 13 x 18 pixels."
        docgetter="getIcon" docsetter="setIcon"
        docdefault="See the API documentation for this component."
        jsxmask="jsxpath"/>
  </record>

  <record include="master.xml" group="css_noclass"/>
  <record include="master.xml" group="interaction_nocursor"/>
  <record include="master.xml" group="access"/>
  <record include="master.xml" group="metadata"/>
</data>
