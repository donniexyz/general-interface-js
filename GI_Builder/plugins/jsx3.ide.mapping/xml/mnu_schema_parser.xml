<data jsxid="jsxroot">
  <record jsxid="1" jsxtext="Add New Rule" jsxtip="appends a new rule to the selected rule">
    <record jsxid="11" jsxtext="Element" jsxtip="appends an 'element' rule" jsxexecute="this.getAncestorOfType(jsx3.ide.mapper.Mapper).doAddChildNode(this.getContextRecordId(),'E');"/>
    <record jsxid="12" jsxtext="Attribute" jsxtip="appends an 'attribute' rule" jsxexecute="this.getAncestorOfType(jsx3.ide.mapper.Mapper).doAddChildNode(this.getContextRecordId(),'A');"/>
    <record jsxid="13" jsxtext="CDATA" jsxtip="appends a 'CDATA' rule" jsxexecute="this.getAncestorOfType(jsx3.ide.mapper.Mapper).doAddChildNode(this.getContextRecordId(),'D');"/>
  </record>
  <record jsxid="clone" jsxtext="Clone" jsxtip="makes an exact copy of this rule and appends as a sibling" jsxexecute="this.getAncestorOfType(jsx3.ide.mapper.Mapper).doCloneNode(this.getContextRecordId());"/>
  <record jsxid="reparse" jsxdivider="1" jsxtext="Reparse..." jsxtip="reparses this branch of the rules tree" jsxexecute="this.getContextParent().getAncestorOfType(jsx3.ide.mapper.Mapper).reparseBranch(this.getContextRecordId());"/>
  <record jsxid="sample" jsxdivider="1" jsxtext="Sample Message..." jsxtip="generates a sample message and displays in a new dialog (applies only to input, output, and fault rules)" jsxexecute="this.getContextParent().getAncestorOfType(jsx3.ide.mapper.Mapper).generateSample();"/>
  <record jsxid="quicktest" jsxtext="Execute (Quick Test)" jsxtip="executes the selected operation (equivalent to calling doCall at runtime)" jsxexecute="this.getContextParent().getAncestorOfType(jsx3.ide.mapper.Mapper).execute(this.getContextRecordId());"/>
</data>
