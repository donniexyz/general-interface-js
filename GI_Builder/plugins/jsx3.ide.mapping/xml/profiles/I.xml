<data jsxid="jsxroot">
    <record jsxid="jsxid" jsxtext="Rule ID"
        jsxtip="unique ID assigned by the system when the rule node was created"
        jsxmask="jsxnull"/>
    <record jsxid="mytype" jsxtext="Rule Type"
        jsxtip="rule node type"
        jsxmask="jsxnull"
        value="Input"/>
    <record jsxid="jsxtext" jsxtext="Node Name"
        jsxtip="The name of the node as it will appear in the XML message that will be created/processed"
        jsxmask="jsxnull"/>
    <record jsxid="path" jsxtext="Source Path"
        jsxtip="'XSL Path' to locate the source node in the pattern master that was used to create the selected rule node"
        jsxmask="jsxTextMask"/>
    <record jsxid="src" jsxtext="Input Source"
        jsxtip="URL to the input pattern master (Schema, XML) used to generate these rules. Does not apply to WSDLs"
        jsxmask="jsxTextMask"/>
  <record jsxid="soapuse" jsxtext="SOAP Use"
      jsxtip="Encode if 'encoded'"
      jsxmask="jsxTextMask"/>
  <record jsxid="soapencstyle" jsxtext="SOAP Encoding"
      jsxtip="Encode if 'http://schemas.xmlsoap.org/soap/encoding/'"
      jsxmask="jsxTextMask"/>
</data>
