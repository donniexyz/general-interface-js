<plugin xmlns="http://www.generalinterface.org/gi/amp"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.generalinterface.org/gi/amp http://www.generalinterface.org/xsd/plugin.xsd"
        id="jsx3.ide.editor.propsbundle"
        name="Properties Bundle Editor Plug-In"
        version="0.1"
        property="PropsBundleEditorPlugin">

  <requires>
    <plugin id="jsx3.ide.editor.props"/>
    <plugin id="jsx3.gui.ImageButton"/>
  </requires>

  <resources>
    <script id="js" path="PropsBundleEditor.js"/>
    <xml id="editor" path="tab_propsbundleeditor.xml"/>
  </resources>

  <extension point="jsx3.ide.editor.type">
    <type class="jsx3.ide.PropsBundleEditor">
      <filetype id="ljss"/>
    </type>
  </extension>
  
</plugin>
    