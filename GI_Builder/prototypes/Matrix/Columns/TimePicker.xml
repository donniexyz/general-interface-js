<serialization xmlns="urn:tibco.com/v3.0">
  <name><![CDATA[Time Picker]]></name>
  <icon><![CDATA[images/prototypes/matrix-columnmask.gif]]></icon>
  <description><![CDATA[Extends the Time column with a TimePicker edit mask.]]></description>
  <object type="jsx3.gui.Matrix.Column">
    <dynamics jsxborder="@Outset" jsxbg="@Bevel TopDown" jsxbgcolor="@Solid Disabled"/>
    <strings jsxname="timeColumn" jsxwidth="100" jsxtext="Time"
        jsxpadding="2" jsxcellpadding="2" jsxpath="time" jsxformathandler="@time,short"
        jsxvaluetemplate="@empty"/>
    <object type="jsx3.gui.TimePicker">
      <strings jsxname="timePicker" jsxbgcolor="#FFFFFF"/>
    </object>
  </object>
</serialization>
